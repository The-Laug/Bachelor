import "setifySum_summationDomain.vpr"



method sumClosedForm(n: Int) returns (res: Int)
  requires 0 <= n
  ensures res == simplesum(0, n)
{
  res := 0
  var i: Int := 0;

  while (i <= n)
    invariant i <= (n + 1)
    invariant res == (i - 1) * i / 2 
  {
    res := res + i
    i := i + 1
  }
}


